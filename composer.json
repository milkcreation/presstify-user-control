{
  "name": "presstify-plugins/user-control",
  "description": "Extension PresstiFy de prise de controle de compte utilisateur.",
  "type": "presstify-plugin",
  "keywords": [
    "presstify",
    "plugins",
    "wordpress",
    "user"
  ],
  "homepage": "https://github.com/milkcreation/presstify-user-control.git",
  "time": "2021-09-14",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy.manner@milkcreation.fr",
      "role": "Developper"
    }
  ],
  "require": {
    "composer/installers": "~1.0",
    "presstify/framework": ">=1.4"
  },
  "autoload": {
    "files": [
      "helpers.php"
    ],
    "psr-4": {
      "tiFy\\Plugins\\UserControl\\": ""
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config" : {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "secure-http": false,
    "sort-packages": true
  }
}
